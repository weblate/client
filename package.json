{
    "name": "neatplus",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@craco/craco": "^7.1.0",
        "@math.gl/web-mercator": "^4.1.0",
        "@uiw/react-textarea-code-editor": "^3.1.0",
        "d3": "^7.9.0",
        "gl-matrix": "^3.4.3",
        "html-react-parser": "^5.1.18",
        "mapbox-gl": "^3.8.0",
        "po2json": "^0.4.5",
        "postcss": "^8.4.49",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-focus-lock": "^2.13.2",
        "react-google-recaptcha-v3": "^1.10.1",
        "react-icons": "^5.3.0",
        "react-inlinesvg": "^4.1.5",
        "react-map-gl": "^7.1.7",
        "react-redux": "^9.1.2",
        "react-router-dom": "^7.0.1",
        "react-scripts": "^5.0.1",
        "recharts": "^2.13.3",
        "redux": "^5.0.1",
        "redux-logger": "^3.0.6",
        "redux-persist": "^6.0.0",
        "reselect": "^5.1.1",
        "sass": "^1.81.0",
        "typescript": "^5.7.2",
        "web-vitals": "^4.2.4",
        "whatwg-fetch": "^3.6.2",
        "yet-another-abortcontroller-polyfill": "^0.0.4"
    },
    "scripts": {
        "start": "craco start",
        "build": "craco build",
        "test": "craco test --testPathIgnorePatterns=src/vendor/",
        "eject": "react-scripts eject",
        "lint": "eslint --fix --ext .js,.jsx . && stylelint '**/*.{css,scss}'",
        "generate:po": "react-gettext-parser --config translation.config.js --output src/po/locale_fr.po 'src/**/{*.js,*.jsx,*.ts,*.tsx}' '!src/vendor'",
        "prepare": "husky && sh scripts/prepare_translations.sh"
    },
    "eslintConfig": {
        "extends": [
            "react-app"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all",
            "not chrome < 51",
            "not safari < 10",
            "not ie 11"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "resolutions": {
        "react-error-overlay": "6.0.11",
        "@svgr/webpack": "6.5.1",
        "fork-ts-checker-webpack-plugin": "^6.5.3"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.25.9",
        "@testing-library/jest-dom": "^6.1.3",
        "babel-eslint": "^10.1.0",
        "babel-plugin-transform-remove-console": "^6.9.4",
        "customize-cra": "^1.0.0",
        "eslint-plugin-css-modules": "^2.12.0",
        "eslint-plugin-react": "^7.32.2",
        "husky": "^9.1.7",
        "lint-staged": "^15.2.10",
        "react-gettext-parser": "^1.16.0",
        "stylelint": "^15.2.0",
        "stylelint-config-standard": "^30.0.1",
        "stylelint-config-standard-scss": "^7.0.1"
    },
    "jest": {
        "moduleNameMapper": {
            "@ra/(.*)": "<rootDir>/src/vendor/react-arsenal/$1",
            "@uiw/react-textarea-code-editor": "<rootDir>/node_modules/@uiw/react-textarea-code-editor/dist/editor.js"
        },
        "transformIgnorePatterns": [
            "<rootDir>/node_modules/rehype"
        ]
    }
}
